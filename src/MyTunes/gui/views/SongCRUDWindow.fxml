<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane id="songPane" prefHeight="350.0" prefWidth="500.0" stylesheets="@../css/Main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyTunes.gui.controllers.SongCRUDWindow">
   <children>
      <VBox layoutX="7.0" prefHeight="350.0" prefWidth="500.0">
         <children>
            <HBox prefHeight="35.0" prefWidth="500.0" />
            <HBox prefHeight="279.0" prefWidth="500.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label text="Title: " textFill="#50f3ff">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label text="Artist: " textFill="#50f3ff">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label text="Category: " textFill="#50f3ff">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="97.0" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label text="Time: " textFill="#50f3ff">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="76.0" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label text="File: " textFill="#50f3ff">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox prefHeight="262.0" prefWidth="394.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                           <children>
                              <TextField fx:id="titleTextField" prefHeight="32.0" prefWidth="280.0">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                           <children>
                              <TextField fx:id="artistTextField" prefHeight="32.0" prefWidth="280.0">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                           <children>
                              <ComboBox fx:id="categorySelection" prefHeight="32.0" prefWidth="193.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                           <children>
                              <TextField fx:id="timeTextField" prefHeight="32.0" prefWidth="110.0">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="374.0" spacing="20.0">
                           <children>
                              <TextField fx:id="fileTextField" prefHeight="32.0" prefWidth="278.0">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </TextField>
                              <Button fx:id="filepathSelection" mnemonicParsing="false" onAction="#selectTheFilePathOfSong" prefHeight="32.0" prefWidth="86.0" text="Choose">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Button>
                           </children>
                           <padding>
                              <Insets right="20.0" />
                           </padding>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="BASELINE_RIGHT" prefHeight="87.0" prefWidth="500.0" spacing="20.0">
               <children>
                  <Button fx:id="cancelSongEdit" mnemonicParsing="false" onAction="#CancelEditOfSong" prefHeight="32.0" prefWidth="86.0" text="Cancel">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="saveSongEdit" mnemonicParsing="false" onAction="#saveEditOfSong" prefHeight="32.0" prefWidth="86.0" text="Save">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets right="20.0" top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
